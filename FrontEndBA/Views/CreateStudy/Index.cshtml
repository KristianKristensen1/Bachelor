@model BachelorBackEnd.Study

@{
    ViewData["Title"] = "Index";
}


<h4>Study</h4>
<hr />
<div class="container">
    <div class="row">
        <div class="col-md-5" id="right" style="background-color: white; overflow-y: scroll; height: 200px; width: 100%; margin-bottom: 20px">
            <h3 class="tableTitle">Create a Study</h3>
            <table class="table">
                <thead>
                <tr>
                    <th class="col-md-1">
                        Study Name
                    </th>
                    <th>
                        Test Study
                    </th>
                </tr>
                <tr>
                    <th class="col-md-1">
                        Study Type
                    </th>
                </tr>
                </thead>
            </table>
        </div>

    </div>
</div>






    <div class="row">
        <div class="col-md-4">
            <form asp-action="Index">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="Description" class="control-label"></label>
                    <input asp-for="Description" class="form-control" />
                    <span asp-validation-for="Description" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <div class="checkbox">
                        <label>
                            <input asp-for="Isdraft" /> @Html.DisplayNameFor(model => model.Isdraft)
                        </label>
                    </div>
                </div>
                <div class="form-group">
                    <label asp-for="Tag" class="control-label"></label>
                    <input asp-for="Tag" class="form-control" />
                    <span asp-validation-for="Tag" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-default" formaction="Create" />
                </div>
            </form>
        </div>
    </div>


